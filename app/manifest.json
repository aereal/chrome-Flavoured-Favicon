{
  "name": "Flavoured Favicon",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Flavour favicons by its domains",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/lib/storage-key.js",
      "js/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "options/options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
