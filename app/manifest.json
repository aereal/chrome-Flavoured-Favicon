{
  "name": "Flavoured Favicon",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Change color tastes of favicons by their domains",
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "options/options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
